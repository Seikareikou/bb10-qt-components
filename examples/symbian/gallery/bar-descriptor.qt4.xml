<?xml version="1.0" encoding="utf-8" standalone="no"?>
<qnx xmlns="http://www.qnx.com/schemas/application/1.0">
    <id>de.zwong.symqtcompgallery.qt4</id>
    <name>Symbian Qt4 Gallery</name>
    <versionNumber>1.0.0</versionNumber>
    <description>Bla bla bla</description>

    <initialWindow>
        <systemChrome>none</systemChrome>
        <transparent>false</transparent>
        <autoOrients>false</autoOrients>
        <aspectRatio>portrait</aspectRatio>
    </initialWindow>

    <env var="QML_IMPORT_PATH" value="app/native/imports"/>-
    <env var="QT_DEBUG_PLUGINS" value="0" />
    <env var="LD_DEBUG" value="0"/>

    <author>Kodira</author>
    <authorId>gYAAgL_PcYLD7BBtneIZf-PifPE</authorId>
    <action system="true">run_native</action>
    <category>core.games</category>

    <!-- Better performance?! -->
    <!-- <env var="QBB_USE_OPENGL" value="1"/> -->

    <asset entry="true" path="qmlgallery" type="Qnx/Elf">qmlgallery</asset>
    <asset path="icon.png">icon.png</asset>

    <asset path="qt4/imports">imports</asset>

    <icon><image>icon.png</image></icon>
</qnx>

